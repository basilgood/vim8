{
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.useQuickfixForLocations": true,
  "dialog.floatBorderHighlight": "FloatBorder",
  "floatFactory.floatConfig": {
    "rounded": true,
    "border": true,
    "borderhighlight": "FloatBorder",
    "maxWidth": 50
  },
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.insertMode": "replace",
  "suggest.detailField": "abbr",
  "signature.target": "echo",
  "diagnostic.displayByAle": true,
  "snippets.ultisnips.enable": false,
  "html.filetypes": [
    "html",
    "htmldjango",
    "astro",
    "jinja"
  ],
  "html-css-support.enabledLanguages": [
    "html",
    "htmldjango",
    "astro",
    "jinja",
    "javascript"
  ],
  "htmldjango.djlint.indent": 2,
  "htmldjango.djhtml.tabWidth": 2,
  "htmldjango.djlint.formatJs": true,
  "htmldjango.djlint.formatCss": true,
  "htmldjango.djlint.preserveBlankLines": true,
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.lens.enable": true,
  "rust-analyzer.updates.prompt": true,
  "rust-analyzer.signatureInfo.detail": "parameters",
  "markdownlint.config": {
    "line-length": false
  }
}
